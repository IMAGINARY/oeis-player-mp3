{
  "extends": ["airbnb"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "d3": true,
    "updateMap": true,
    "clearGeodesic": true,
    "clearLoxodrome": true,
    "clearEllipses": true,
    "undoCommand": true,
    "projection": true,
    "rotateToCurrentLocation": true,
    "sampleEllipses": true
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline", {"functions": "never"}]
  }
}